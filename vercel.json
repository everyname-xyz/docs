{
  "rewrites": [
    {
      "source": "/openapi.yaml/ui/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/api",
      "has": [
        {
          "type": "host",
          "value": "*.everyname.xyz"
        }
      ]
    },
    {
      "source": "/openapi.yaml/ui/(.*)",
      "destination": "/api",
      "has": [
        {
          "type": "host",
          "value": "*.everyname.xyz"
        }
      ]
    }
  ]
}
